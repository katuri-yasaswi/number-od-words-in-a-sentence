#include <stdio.h>
#include<conio.h>
int prime(int);
int prime(int a)
{
    if(a==1)
    {
        return;
    }
    int i=2;
    while(a%i!=0)
    {
        i++;
    }
    printf("%d\t",i);
    prime(a/i);
}
void main()
{
    int a;
    printf("enter a numner:");
    scanf("%d",&a);
    prime(a);
}

Output:

enter a numner:100
2       2       5       5




