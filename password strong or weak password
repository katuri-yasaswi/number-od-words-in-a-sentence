#include <stdio.h>
#include<string.h>
void main()
{
    char a[20];
    printf("enter a password:");
    scanf("%s",a);
    int len=strlen(a);
    int l=0,s=0,u=0,d=0;
    if(len>7)
    {
      for(int i=0;i<len;i++)
      {
         if(isupper(a[i]))
        u++;
        else if(islower(a[i]))
        l++;
        else if(isdigit(a[i]))
        d++;
        else
        s++;
      }

       if(l>0&&s>0&&u&&d>0)
       {
           printf("STRONG PASSWORD");
       }


   else
   {
       printf("WEAK PASSWORD");
   }
    }
   else
   printf("WEAK DUE TO LESS CHARACTERS");
    }

Output:

enter a password:Yasaswi@h5
STRONG PASSWORD
